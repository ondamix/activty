#include <stdio.h>



int main(){

    char name;
    int age=0;

    printf("Name:");
    scanf("%s", &name);

    printf("Age:");
    scanf("%d", &age);

    if(!age!=0){
        printf("\nInvalid age, Please Try Again.");
    }
     else if(age>=18){

        printf("\nYou are eligible for voting.");
    }
    else{

        printf("\nYou are not eligible for vote.");
    }
return 0;
}


